/*导航栏*/
.class-bar-wapper {
  display: flex;
  height: 80rpx;
  width: 1000rpx;
  background-color: rgb(255, 250, 229);
}

.classes-wapper {
  width: 400rpx;
  height: 70rpx;
  margin: 10rpx;
  align-items: center;
  text-align: center;
}

.class-title {
  font-size: 35rpx;
  line-height: 70rpx;
  color: #fd5523;
  font-weight: bold;
  font-family: Arial, Helvetica, sans-serif;
}

/*轮播图*/

.lateast-news-wapper {
  float: center;
  text-align: center;
  width: 100%;
  background-color: rgb(255, 250, 229);
}

swiper {
  height: 350rpx;
  width: 100%;
}

swiper-item {
  position: absolute;
  width: 100%;
  height: 100%;
}

/*新闻列表*/
.news-list-wapper{
  background-color: rgb(255, 250, 229);
  height: 776rpx;
}